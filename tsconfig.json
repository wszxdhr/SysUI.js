{
  "version": "1.8.0",
  "compilerOptions": {
    "outDir": "dist/compiled",
    "lib": [
      "es6",
      "es5",
      "dom"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "declaration": true
  },
  "exclude": [ "build", "dist", "node_modules" ]
}
